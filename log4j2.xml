<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console">
			<PatternLayout pattern="[%d{HH:mm:ss}] [%t/%level]: %msg%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="OFF">
			<AppenderRef ref="Console"/>
		</Root>
	</Loggers>
</Configuration>
